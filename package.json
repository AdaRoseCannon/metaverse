{
	"name": "metaverse",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"dependencies": {
		"bufferutil": "^1.3.0",
		"express": "^4.14.0",
		"utf-8-validate": "^1.2.1",
		"ws": "^1.1.1"
	},
	"devDependencies": {
		"babel-cli": "^6.18.0",
		"babel-preset-env": "^1.1.4",
		"nodemon": "^1.11.0"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "./node_modules/.bin/babel ./static/scripts/ -d ./static/build/",
		"start": "node .",
		"watch": "nodemon -w \"./static/scripts/main.es6\" -x 'npm run build'"
	},
	"author": "",
	"license": "MIT",
	"eslintConfig": {
		"extends": [
			"plugin:import/errors"
		],
		"parserOptions": {
			"sourceType": "module"
		},
		"env": {
			"browser": true,
			"mocha": true,
			"node": true
		},
		"rules": {
			"no-unused-vars": 2,
			"no-undef": 2,
			"eqeqeq": 2,
			"no-underscore-dangle": 0,
			"guard-for-in": 2,
			"no-extend-native": 2,
			"wrap-iife": 2,
			"new-cap": 2,
			"no-caller": 2,
			"quotes": [
				1,
				"single"
			],
			"indent": [
				"error",
				"tab"
			],
			"no-loop-func": 2,
			"no-irregular-whitespace": 2,
			"no-multi-spaces": 2,
			"one-var": [
				2,
				"never"
			],
			"no-var": 1,
			"strict": [
				1,
				"global"
			],
			"no-console": 1
		},
		"root": true
	}
}
